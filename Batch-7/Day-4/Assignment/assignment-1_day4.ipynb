   ],
   "source": [
    "n = range(150, 200)\n",
    "for num in n:\n",
    "    sum = 0\n",
    "    temp = num\n",
    "    while temp > 0:\n",
    "        digit = temp % 10\n",
    "        sum += digit ** 3\n",
    "        temp //= 10\n",
    "    if num == sum:\n",
    "        print(num, \"is an armstrong number\")\n",
    "        break"
   ]
  }
